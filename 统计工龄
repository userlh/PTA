#include <bits/stdc++.h>

using namespace std;

main(){
    int n;
    int a[100000]={0},b[100000]={0};
    cin>>n;
    for(int i = 0;i < n;i++){
        cin>>a[i];
    }
    for(int i = 0;i < n;i++){
        b[a[i]]++;
    }
    for(int i = 0; i <= 50;i++){
        if(b[i] != 0)
            cout<<i<<":"<<b[i]<<endl;
    }


    return 0;
}
