#include<bits/stdc++.h>
using namespace std;
int main()
{
    string a;
    getline(cin,a);
    string b;
    getline(cin,b);
    int lena = a.size();
    int lenb = b.size();
    int i,j;
    for(i = 0;i < lena;i++){
        int flag=0;
        for(j = 0;j < lenb;j++){
            if(a[i] == b[j]){
               flag = 1;
            }
        }
        if(flag == 0){
            printf("%c",a[i]);
        }
    }

    return 0;
}
